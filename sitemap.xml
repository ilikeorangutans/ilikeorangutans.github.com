{% include JB/setup %}
{% assign posts_collate = site.posts %}
<xml version="1.0" encoding="UTF-8" ?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	{% for page in site.pages %}
	<url>
		<loc>{{ page.url }}</loc>
		<changefreq>monthly</changefreq>
	</url>{% endfor %}
	{% for post in posts_collate %}
	<url>
		<loc>{{ post.url }}</loc>
		<changefreq>monthly</changefreq>
	</url>{% endfor %}
</urlset>
